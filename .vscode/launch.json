    "version": "0.2.0",
    "configurations": [
        {
            "name": "prog1_mandelbrot_threads",
            "type": "cppdbg",
            "request": "launch",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/prog1_mandelbrot_threads",
            "program": "${cwd}/prog1_mandelbrot_threads/mandelbrot",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "prog1_mandelbrot_threads",
        },
